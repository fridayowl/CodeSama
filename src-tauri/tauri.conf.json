{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run start",
    "devPath": "http://localhost:3000",
    "distDir": "../build"
  },
  "identifier": "com.tauri.code01",
  "productName": "code01",
  "version": "0.1.1",
  "plugins": {
    "shell": {
      "open": true
    }
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 1000,
        "resizable": false,
        "title": "code01",
        "width": 1280
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": ["deb", "msi", "dmg", "app"]
  },
  "security": {
    "csp": null
  }
  
}
